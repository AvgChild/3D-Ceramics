{
  "name": "clay-archive-uploader",
  "version": "1.0.0",
  "description": "Automated uploader for Clay Archive 3D models",
  "scripts": {
    "build": "rm -rf public && mkdir -p public && cp -r css js catalog *.html public/",
    "start": "echo 'Static site - see public/ directory'",
    "upload": "node scripts/upload-model.js",
    "upload:batch": "node scripts/batch-upload.js",
    "validate-catalog": "node scripts/validate-catalog.js",
    "sync-catalog": "node scripts/sync-catalog.js",
    "sync-catalog:new-only": "node scripts/sync-catalog.js --new-only"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.6.1",
    "glob": "^10.4.5",
    "inquirer": "^8.2.7",
    "mime-types": "^2.1.35",
    "sharp": "^0.32.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
